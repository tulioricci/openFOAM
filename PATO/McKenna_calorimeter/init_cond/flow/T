FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0/flow";
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   uniform 300;

boundaryField
{
    burner
    {
        type            fixedValue;
        value           uniform 300;
    }

    outlet
    {
        type            inletOutlet; 
        inletValue      uniform 300;
    }

    farfield
    {
        type            inletOutlet; 
        inletValue      uniform 300;
    }

    f_front
    {
        type            wedge;
    }

    f_back
    {
        type            wedge;
    }

    fuel
    {
        type            fixedValue;
        value           uniform 300;
    }

    shield
    {
        type            fixedValue;
        value           uniform 300;
    }

    flow_to_body
    {
        //type            compressible::turbulentTemperatureRadCoupledMixed;
        //qrNbr           Qr;
        type            compressible::turbulentTemperatureCoupledBaffleMixed;
        Tnbr            Ta;
        value           uniform 300;
        kappaMethod     fluidThermo;
    }

    flow_to_holder
    {
        //type            compressible::turbulentTemperatureRadCoupledMixed;
        //qrNbr           Qr;
        type            compressible::turbulentTemperatureCoupledBaffleMixed;
        Tnbr            Ta;
        value           uniform 300;
        kappaMethod     fluidThermo;
    }

    flow_to_slug
    {
        //type            compressible::turbulentTemperatureRadCoupledMixed;
        //qrNbr           Qr;
        type            compressible::turbulentTemperatureCoupledBaffleMixed;
        Tnbr            Ta;
        value           uniform 300;
        kappaMethod     fluidThermo;
    }
}


