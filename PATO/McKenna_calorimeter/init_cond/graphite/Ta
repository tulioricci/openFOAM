FoamFile
{
    version         2.0;
    format          ascii;
    class           volScalarField;
    object          T;
}

dimensions      [ 0 0 0 1 0 0 0 ];

internalField   uniform 300;

boundaryField
{
   
    graphite_to_holder
    {   
        type            compressible::turbulentTemperatureCoupledBaffleMixed;
        value           uniform 300;
        Tnbr 		Ta;
        kappaMethod     lookup;
        kappa		k_abl_sym;
    }

    graphite_to_body
    {   
        type            compressible::turbulentTemperatureCoupledBaffleMixed;
        value           uniform 300;
        Tnbr 		Ta;
        kappaMethod     lookup;
        kappa		k_abl_sym;
    }

    g_back
    {
        type            wedge;
    }

    g_front
    {
        type            wedge;
    }
}


