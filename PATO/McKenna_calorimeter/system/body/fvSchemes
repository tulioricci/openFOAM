FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system/slug";
    object      fvSchemes;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

ddtSchemes {
default            Euler; // backward;
}

gradSchemes {
default           Gauss linear corrected;
}

divSchemes {
default           Gauss  linear corrected;
}

laplacianSchemes {
default           Gauss linear corrected;
}

interpolationSchemes {
default         linear;
}

snGradSchemes {
default         corrected;
}


