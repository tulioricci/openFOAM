FoamFile
{
    version     2.0;
    format      binary;
    class       volVectorField;
    location    "0.000000";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField  uniform (0.290341017059834 0. 0.);

 
boundaryField
{
    f_frontAndBack
    {
        type            empty;
    }
    
    f_side
    {
        type            slip;
    }
    
    fluid_to_solid
    {
        type            noSlip;
    }
    
    outlet
    {
        type            inletOutlet;
        inletValue      uniform (0.8710230511795021 0 0 );
    }
    
    inlet
    {
        type      fixedValue;
        value     uniform (0.290341017059834 0. 0.);
    }

}


