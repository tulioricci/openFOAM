FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0/fluid";
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 101325.0;

boundaryField
{
    
    fluid_to_solid
    {
        type            zeroGradient;
    }
    
    f_side
    {
        type            zeroGradient;
    }

    inlet
    {
        type            fixedFluxExtrapolatedPressure;
    }

    outlet
    {
        type            waveTransmissive;
        gamma           1.4;
        fieldInf        101325.0;
        lInf            0.005;
        //type            fixedValue;
        value           uniform 101325.0;
    }

    f_frontAndBack
    {
        type            empty;
    }
}


