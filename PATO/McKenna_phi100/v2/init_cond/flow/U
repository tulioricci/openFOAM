FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "0.000000";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    f_front
    {
        type            wedge;
    }
    outlet
    {
        type            inletOutlet;
        inletValue      uniform ( 0 0 0 );
        //type            pressureInletOutletVelocity;
        //value           uniform (0 0 0);
        //type            pressureDirectedInletOutletVelocity;
        //inletDirection  uniform (0 -1 0);
        //value           uniform (0 0 0);
    }
    f_back
    {
        type            wedge;
    }
    fuel
    {
        //type            fixedValue;
        //value           uniform (0 0.11 0);
        type uniformFixedValue;
        uniformValue table
        (
        (0.0 (0 0.02 0))
        (4.0 (0 0.10 0))
        );
    }
    flow_to_porousMat
    {
        type            fixedValueToNbrValue;
        nbr             U;
        value	        uniform ( 0 0 0 );
    }
    flow_to_graphite
    {
        type            noSlip;
    }
    burner
    {
        type            noSlip;
    }
    shield
    {
/*        type            fixedValue;*/
/*        value           uniform (0 0.10 0);*/
        type uniformFixedValue;
        uniformValue table
        (
        (0.0 (0 0.02 0))
        (4.0 (0 0.10 0))
        );
    }
}


