FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0/flow";
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 101325.0;

boundaryField
{
    fuel
    {
        type            fixedFluxExtrapolatedPressure;
    }

    shield
    {
        type            fixedFluxExtrapolatedPressure;
    }

    outlet
    {
        type            calculated;
        value           uniform 101325.0;
    }

    farfield
    {
        $outlet;
    }

    burner
    {
        type            fixedFluxExtrapolatedPressure;
    }

    f_front
    {
        type            wedge;
    }

    f_back
    {
        type            wedge;
    }

    flow_to_porousMat
    {
        type            fixedFluxExtrapolatedPressure;
    }

    flow_to_graphite
    {
        type            fixedFluxExtrapolatedPressure;
    }
}


