FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0/solid";
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [ 1 -1 -2 0 0 0 0 ];

internalField   uniform 101325.0;

boundaryField
{
    s_back
    {
        type            wedge;
    }

    s_front
    {
        type            wedge;
    }

    porousMat_to_flow
    {
        type            fixedValueToNbrValue;
        nbr             p_rgh;
        //type            fixedValue;
        //value 	        $internalField;
    }

    porousMat_to_alumina
    {
        type            zeroGradient;
    }

    porousMat_to_graphite
    {
        type            zeroGradient;
    }
}


