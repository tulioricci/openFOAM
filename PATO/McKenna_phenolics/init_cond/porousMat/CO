FoamFile {
  version     2.0;
  format      ascii;
  class       volScalarField;
  location    "0";
  object      CO;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0;

boundaryField {
    porousMat_to_flow
    {
        type            fixedValueToNbrValue;
        nbr             CO;
        value           $internalField;
    }
  
    s_back
    {
        type            wedge;
    }

    s_front
    {
        type            wedge;
    }

    porousMat_to_alumina
    {
        type            zeroGradient;
    }

    porousMat_to_graphite
    {
        type            zeroGradient;
    }
}



