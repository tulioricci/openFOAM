FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      CH3;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0.0;

boundaryField
{
    fuel
    {
        type            fixedValue;
        value           uniform 2.5175672311079904e-05;
    }

    outlet
    {
        type            zeroGradient;
    }

    shield
    {
        type            fixedValue;
        value           uniform 0.0;
    }

    burner
    {
        type            zeroGradient;
    }

    flow_to_porousMat
    {
        type            zeroGradient;
    }

    flow_to_graphite
    {
        type            zeroGradient;
    }

    f_front
    {
        type            wedge;
    }

    f_back
    {
        type            wedge;
    }
}
