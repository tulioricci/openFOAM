FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0/fluid";
    object      CH4;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0;

boundaryField
{

    fuel
    {
        type            fixedValue;
        value           uniform 0.05;
    }

    shield
    {
        type            fixedValue;
        value           uniform 0.0;
    }

    outlet
    {
        type            zeroGradient;
    }

    burner
    {
        type            zeroGradient;
    }

    f_front
    {
        type            symmetry;
    }

    f_back
    {
        type            symmetry;
    }

    defaultFaces
    {
        type            empty;
    }

    fluid_to_solid
    {
        type            zeroGradient;
    }
}


