FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0/fluid";
    object      cellToRegion;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    f_front
    {
        type            zeroGradient;
    }
    outlet
    {
        type            zeroGradient;
    }
    f_back
    {
        type            zeroGradient;
    }
    fuel
    {
        type            zeroGradient;
    }
    burner
    {
        type            zeroGradient;
    }
    shield
    {
        type            zeroGradient;
    }
    defaultFaces
    {
        type            zeroGradient;
    }
    fluid_to_solid
    {
        type            calculated;
        value           uniform 0;
    }
}


