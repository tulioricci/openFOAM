FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0/solid";
    object      cellToRegion;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 1;

boundaryField
{
    s_front
    {
        type            zeroGradient;
    }
    s_back
    {
        type            zeroGradient;
    }
    defaultFaces
    {
        type            zeroGradient;
    }
    solid_to_fluid
    {
        type            calculated;
        value           uniform 0;
    }
}


