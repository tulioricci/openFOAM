FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     reactingFoam;

startFrom       latestTime;
/*startFrom       startTime;*/

startTime       0;

stopAt          endTime;

endTime         2;

/*maxDeltaT       666;*/
deltaT          2e-4;

/*writeControl    adjustableRunTime;*/
writeControl    timeStep;

/*writeInterval   1e-3;*/
writeInterval   25;

purgeWrite      0;

writeFormat     ascii;

writePrecision  9;

writeCompression off;

timeFormat      fixed;

timePrecision   6;

runTimeModifiable true;

adjustTimeStep  yes;

maxCo           3.0;

