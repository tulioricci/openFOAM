FoamFile
{
    format      ascii;
    class       volVectorField;
    location    "0/fluid";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    f_frontAndBack
    {
        type            symmetry;
    }
    burner
    {
        type            noSlip;
    }
    outlet
    {
        type            zeroGradient;
    }
    f_axis
    {
        type            slip;
    }
    fuel
    {
        type            fixedValue;
        value           uniform (0 0.11 0);
    }
    shield
    {
        type            fixedValue;
        value           uniform (0 0.11 0);
    }
    fluid_to_solid
    {
        type            noSlip;
    }
}


