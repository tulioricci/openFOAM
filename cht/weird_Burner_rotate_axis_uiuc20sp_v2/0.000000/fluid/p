FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0/fluid";
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 100000.0;

boundaryField
{
    f_frontAndBack
    {
        type            symmetry;
    }

    fuel
    {
        type            zeroGradient; //fixedFluxExtrapolatedPressure;
    }

    shield
    {
        type            zeroGradient;
    }

    outlet
    {
        type            fixedValue;
        value           uniform 100000.0;
    }

    burner
    {
        type            zeroGradient;
    }

    f_axis
    {
        type            zeroGradient;
    }

    fluid_to_solid
    {
        type            zeroGradient;
    }
}


