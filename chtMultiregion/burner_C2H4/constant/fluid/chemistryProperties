FoamFile
{
    format      ascii;
    class       dictionary;
    location    "constant";
    object      chemistryProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

chemistryType
{
    solver          EulerImplicit;
}

chemistry       on;

initialChemicalTimeStep 1e-08;

odeCoeffs
{
    solver          seulex;
    absTol          1e-8;
    relTol          1e-3;

}

EulerImplicitCoeffs
{
    cTauChem        1;
}

