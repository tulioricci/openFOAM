FoamFile
{
    version     2.0;
    format      binary;
    class       volVectorField;
    location    "0.000000";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    f_front
    {
        type            wedge;
    }
    outlet
    {
        type            inletOutlet;
        inletValue      uniform ( 0 0 0 );
    }
    farfield
    {
        type            inletOutlet;
        inletValue      uniform ( 0 0 0 );
    }
    f_back
    {
        type            wedge;
    }
    fuel
    {
        type      fixedValue;
        value     (0 0.145170508529917 0);
        //type uniformFixedValue;
        //uniformValue table
        //(
        //(0.0 (0 0.10 0))
        //(0.2 (0 0.145170508529917 0))
        //);
    }
    sample
    {
        type            noSlip;
    }
    plate
    {
        type            slip;
    }
    burner
    {
        type            noSlip;
    }
    shield
    {
        type      fixedValue;
        value     (0 0.145170508529917 0);
        //type uniformFixedValue;
        //uniformValue table
        //(
        //(0.0 (0 0.10 0))
        //(0.2 (0 0.145170508529917 0))
        //);
    }
}


