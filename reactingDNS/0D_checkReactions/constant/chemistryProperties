FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      chemistryProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

chemistryType
{
    //solver          EulerImplicit;
    chemistrySolver   ode;
    //chemistryThermo   psi;
}

chemistry       on;

initialChemicalTimeStep 1e-13;

EulerImplicitCoeffs
{
    cTauChem        1;
    equilibriumRateLimiter off;
}

odeCoeffs
{
    solver          RKCK45;
    //solver          Trapezoid;
    //solver          rodas34;
    //solver          seulex;
    absTol          1e-15;
    relTol          1e-15;
}

#include "reactions"

// ************************************************************************* //

differentialDiffusion on;
thermalDiffusion off;

Prt  Prt [0 0 0 0 0] 0.0;
Sct  Sct [0 0 0 0 0] 0.0;
