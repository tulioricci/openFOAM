FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      fvOptions;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

fixedTemperature
{
    type            fixedTemperatureConstraint;
    selectionMode   all;
    mode            uniform;
    temperature     table
    (
        (0 300.)
        (50e-8 2500.)   
    );
}
