FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     reactingFoam;

startFrom       latestTime;
/*startFrom       startTime;*/

startTime       0;

stopAt          endTime;

endTime         2;

deltaT          5e-5;

/*writeControl    adjustableRunTime;*/
writeControl    timeStep;

/*writeInterval   1e-3;*/
writeInterval   50;

purgeWrite      0;

writeFormat     binary;

writePrecision  9;

writeCompression off;

timeFormat      fixed;

timePrecision   6;

runTimeModifiable true;

adjustTimeStep  yes;

/*maxCo           5.0;*/

