FoamFile
{
    format      ascii;
    class       volVectorField;
    location    "0/fluid";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    burner
    {
        type            noSlip;
    }
    wall
    {
        type            noSlip;
    }
    outlet
    {
        type            zeroGradient;
    }
    f_front
    {
        type            wedge;
    }
    f_back
    {
        type            wedge;
    }
    fuel
    {
        type            fixedValue;
        value           uniform (0 0.66 0);
    }
    shield
    {
        type            fixedValue;
        value           uniform (0 0.11 0);
    }
}


// ************************************************************************* //
