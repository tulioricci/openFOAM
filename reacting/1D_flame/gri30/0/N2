FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      N2;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0.7247731344409141;

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           uniform 0.7247731344409141;
    }

    outlet
    {
        type            zeroGradient;
    }

    frontAndBack
    {
        type            zeroGradient;
    }

    wall
    {
        type            zeroGradient;
    }
    
}

